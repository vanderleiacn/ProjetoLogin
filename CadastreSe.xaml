<Window x:Class="ProjetoLogin.CadastreSe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetoLogin"
        mc:Ignorable="d"
        Title="Cadastre_se" Height="450" Width="800" >
    <Grid>
        <Label x:Name="lblLogin" Content="Login" HorizontalAlignment="Left" Margin="110,95,0,0" VerticalAlignment="Top" Height="28" Width="76"/>
        <Label x:Name="lblSenha" Content="Senha" HorizontalAlignment="Left" Margin="112,170,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblConfitmaSenha" Content="Confirmar Senha" HorizontalAlignment="Left" Margin="109,218,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TxbLogin" HorizontalAlignment="Left" Margin="113,128,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnCadastrar" Content="Cadastrar" HorizontalAlignment="Left" Margin="131,275,0,0" VerticalAlignment="Top" Width="71"/>
        <PasswordBox x:Name="PsbxSenha" HorizontalAlignment="Left" Margin="114,195,0,0" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="PsbxConfirmaSenha" HorizontalAlignment="Left" Margin="112,244,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
